{
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "noFallthroughCasesInSwitch": true,
    "jsx": "react-jsx",
    "types": ["vite/client", "vite-plugin-svgr/client"]
  },
  "include": [
    "src",
    "public/strategy/division-multitape.ts",
    "public/strategy/division-singletrack.ts",
    "public/strategy/factorial-multitape.ts",
    "public/strategy/factorial-multitrack.ts",
    "public/strategy/multiplication-multitape.ts",
    "public/strategy/multiplication-singletrack.ts",
    "public/strategy/power-multitape.ts",
    "public/strategy/power-multitrack.ts"
  ]
}
